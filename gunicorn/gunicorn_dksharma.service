[Unit]
Description=gunicorn daemon
After=network.target

[Service]
User=ec2-user
Group=nginx
WorkingDirectory=/home/ec2-user/dksharma
ExecStart=/home/ec2-user/dksharma/venv/bin/gunicorn \
          --access-logfile - \
          --workers 3 \
          --bind 127.0.0.1:8001 \
          dksharma.wsgi:application

[Install]
WantedBy=multi-user.target